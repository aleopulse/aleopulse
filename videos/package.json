{
  "name": "leopulse-videos",
  "version": "1.0.0",
  "description": "LeoPulse demo videos",
  "scripts": {
    "dev": "remotion studio",
    "build": "remotion bundle",
    "render": "remotion render DemoVideo out/demo.mp4",
    "render:gif": "remotion render DemoVideo out/demo.gif --image-format=png",
    "render:pitchdeck": "remotion render PitchDeck out/pitchdeck.mp4",
    "render:pitchdeck:gif": "remotion render PitchDeck out/pitchdeck.gif --image-format=png",
    "thumbnail": "remotion still DemoVideo out/thumbnail.png --frame=0",
    "thumbnail:pitchdeck": "remotion still PitchDeck out/pitchdeck-thumbnail.png --frame=0",
    "upgrade": "remotion upgrade",
    "lint": "eslint src && tsc",
    "generate:voiceover": "npx tsx scripts/generate-voiceover.ts",
    "generate:voiceover:pitchdeck": "npx tsx scripts/generate-pitchdeck-voiceover.ts"
  },
  "repository": {},
  "license": "UNLICENSED",
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.32.0",
    "@remotion/cli": "^4.0.0",
    "dotenv": "^17.2.3",
    "openai": "^6.16.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "remotion": "^4.0.0"
  },
  "devDependencies": {
    "@remotion/eslint-config-flat": "^4.0.0",
    "@types/react": "19.2.7",
    "@types/web": "0.0.166",
    "eslint": "9.19.0",
    "prettier": "3.6.0",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  },
  "private": true
}
